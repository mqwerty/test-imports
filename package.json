{
  "name": "@mqwerty/test-imports",
  "version": "1.0.0",
  "license": "UNLICENSED",
  "private": true,
  "type": "module",
  "engines": {
    "node": ">=20",
    "npm": ">=10"
  },
  "scripts": {
    "start:dev": "node --enable-source-maps --import @swc-node/register/esm-register ./src/index.ts",
    "start:build": "node --enable-source-maps ./build/index.js",
    "prebuild": "rm -rf build",
    "build": "tsc",
    "typecheck": "tsc --noEmit"
  },
  "imports": {
    "#utils": "./build/utils/index.js",
    "#utils/*": "./build/utils/*"
  },
  "devDependencies": {
    "@swc-node/register": "1.9.1",
    "@types/node": "20.12.7",
    "prettier": "3.2.5",
    "typescript": "5.4.5"
  }
}
